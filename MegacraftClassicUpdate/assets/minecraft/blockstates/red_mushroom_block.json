{
    "multipart": [
        {   "when": { "north": true },
            "apply": [
                { "model": "block/red_mushroom_block" },
                { "model": "block/red_mushroom_block1" },
                { "model": "block/red_mushroom_block2" },
                { "model": "block/red_mushroom_block3" },
                { "model": "block/red_mushroom_block4" }
              ]
        },
        {   "when": { "east": true },
            "apply": [
                { "model": "block/red_mushroom_block", "y": 90, "uvlock": true },
                { "model": "block/red_mushroom_block1", "y": 90, "uvlock": true },
                { "model": "block/red_mushroom_block2", "y": 90, "uvlock": true },
                { "model": "block/red_mushroom_block3", "y": 90, "uvlock": true },
                { "model": "block/red_mushroom_block4", "y": 90, "uvlock": true }
              ]
        },
        {   "when": { "south": true },
            "apply": [
                { "model": "block/red_mushroom_block", "y": 180, "uvlock": true },
                { "model": "block/red_mushroom_block1", "y": 180, "uvlock": true },
                { "model": "block/red_mushroom_block2", "y": 180, "uvlock": true },
                { "model": "block/red_mushroom_block3", "y": 180, "uvlock": true },
                { "model": "block/red_mushroom_block4", "y": 180, "uvlock": true }
              ]
        },
        {   "when": { "west": true },
            "apply": [
                { "model": "block/red_mushroom_block", "y": 270, "uvlock": true },
                { "model": "block/red_mushroom_block1", "y": 270, "uvlock": true },
                { "model": "block/red_mushroom_block2", "y": 270, "uvlock": true },
                { "model": "block/red_mushroom_block3", "y": 270, "uvlock": true },
                { "model": "block/red_mushroom_block4", "y": 270, "uvlock": true }
              ]
        },
        {   "when": { "up": true },
            "apply": [
                { "model": "block/red_mushroom_block", "x": 270, "uvlock": true },
                { "model": "block/red_mushroom_block1", "x": 270, "uvlock": true },
                { "model": "block/red_mushroom_block2", "x": 270, "uvlock": true },
                { "model": "block/red_mushroom_block3", "x": 270, "uvlock": true },
                { "model": "block/red_mushroom_block4", "x": 270, "uvlock": true }
              ]
        },
        {   "when": { "down": true },
            "apply": [
                { "model": "block/red_mushroom_block", "x": 90, "uvlock": true },
                { "model": "block/red_mushroom_block1", "x": 90, "uvlock": true },
                { "model": "block/red_mushroom_block2", "x": 90, "uvlock": true },
                { "model": "block/red_mushroom_block3", "x": 90, "uvlock": true },
                { "model": "block/red_mushroom_block4", "x": 90, "uvlock": true }
              ]
        },
        {   "when": { "north": false },
            "apply": { "model": "block/mushroom_block_inside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "block/mushroom_block_inside", "y": 90, "uvlock": false }
        },
        {   "when": { "south": false },
            "apply": { "model": "block/mushroom_block_inside", "y": 180, "uvlock": false }
        },
        {   "when": { "west": false },
            "apply": { "model": "block/mushroom_block_inside", "y": 270, "uvlock": false }
        },
        {   "when": { "up": false },
            "apply": { "model": "block/mushroom_block_inside", "x": 270, "uvlock": false }
        },
        {   "when": { "down": false },
            "apply": { "model": "block/mushroom_block_inside", "x": 90, "uvlock": false }
        }
    ]
}
